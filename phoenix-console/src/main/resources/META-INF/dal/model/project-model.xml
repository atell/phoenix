<?xml version="1.0" encoding="UTF-8"?>
<model model-package="com.dianping.phoenix.project" enable-sax-parser="true">
	<entity name="root" root="true">
		<entity-ref name="bussinessLine" type="map" names="bussinessLines" />
	</entity>
	<entity name="bussinessLine">
		<attribute name="name" value-type="String" key="true" />
	</entity>
	<entity name="bussinessLine">
		<entity-ref name="project" type="map" names="projects" />
	</entity>
	<entity name="project">
		<attribute name="name" value-type="String" key="true" />
		<entity-ref name="host" type="list" names="hosts" xml-indent="true" />
	</entity>
	<entity name="host">
		<element name="agentStatus" value-type="String" render="false"/>
		<element name="agentVersion" value-type="String" render="false"/>
		<element name="container" value-type="com.dianping.phoenix.agent.response.entity.Container" render="false"/>
		<element name="domain" names="domains" value-type="com.dianping.phoenix.agent.response.entity.Domain" type="list" render="false"/>
  	</entity>
</model>

