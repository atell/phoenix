<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">

	<filter>
		<filter-name>DemoFilter0</filter-name>
		<filter-class>com.dianping.kernel.demo.DemoFilter</filter-class>
	</filter>
	<filter>
		<filter-name>DemoFilter1</filter-name>
		<filter-class>com.dianping.kernel.demo.DemoFilter</filter-class>
	</filter>
	<filter>
		<filter-name>DemoFilter2</filter-name>
		<filter-class>com.dianping.kernel.demo.DemoFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>DemoFilter0</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>DemoFilter1</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>DemoFilter2</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<listener>
		<listener-class>com.dianping.kernel.demo.DemoListener</listener-class>
	</listener>
</web-app>

